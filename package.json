{
  "name": "david-norman-website",
  "private": true,
  "description": "A website build in React with Gatsby. Sorta.",
  "version": "0.0.1",
  "author": "David Norman <rockbigdave@gmail.com>",
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby": "^2.24.9",
    "gatsby-cli": "^2.12.65",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.4.20",
    "gatsby-plugin-offline": "^3.2.20",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-sass": "^2.1.29",
    "gatsby-plugin-sharp": "^2.6.21",
    "gatsby-source-filesystem": "^2.3.22",
    "gatsby-transformer-remark": "^2.8.27",
    "gatsby-transformer-sharp": "^2.5.3",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-typescript": "^2.4.16",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx && npm run lint-sass",
    "lint:fix": "npm run lint --fix",
    "lint-legacy": "eslint --ext .tsx .",
    "lint-sass": "npx stylelint src/**/*.scss",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "type-check": "tsc",
    "update-deps-naughty": "npm update --save/--save-dev -f"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidn0rman/website.git"
  },
  "bugs": {
    "url": "https://github.com/davidn0rman/website/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint",
      "...": "..."
    }
  }
}
